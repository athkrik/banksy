<!doctype html>
<html class="no-js" lang="en">
    <head>
        <meta charset="utf-8">
        <title>{% block page_title %}{% endblock %} | Meet-Up Event Planner</title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">
    
        <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css">
        <link rel="stylesheet" href="assets/css/main.css">

    </head>
    <body>
        <header class="site-header">
            <div class="wrapper">
                <a href="index.html" class="nav-open">
                    <span class="sr-only">Open Navigation</span>
                    <i class="fa fa-bars"></i>
                </a>
                <h1 class="site-title"><a href="index.html">Meet-Up Event Planner</a></h1>

                <nav class="site-nav">
                    <ul>
                        <li>
                            <a href="index.html" class="nav-close">
                                <span class="sr-only">Close Navigation</span>
                                <i class="fa fa-times"></i>
                            </a>
                        </li>
                        <li><a href="index.html">Register</a></li>
                        <li><a href="create.html">Create an Event</a></li>
                        <li><a href="events.html">My Events</a></li>
                    </ul>
                </nav>
            </div>
        </header>

        <main class="site-main">
            <div class="wrapper">
                {% block content %}{% endblock %}
            </div>
        </main>

        
        <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBWbn2woYcKBa78EGto81shmbogj-9LfF0&amp;libraries=places&amp;callback=initAutocomplete" aync defer></script>

        <script src="assets/js/main.js"></script>
        {% block scripts %}{% endblock %}

    </body>
</html>
